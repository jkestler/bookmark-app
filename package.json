{
  "name": "bookmark-app",
  "version": "1.0.0",
  "description": "A bookmark organizer for developers.",
  "main": "src/server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "test": "export NODE_ENV=test && jasmine",
    "start": "node src/server.js",
    "server": "nodemon src/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "author": "Jonathan Kestler",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.0",
    "express-flash": "0.0.2",
    "express-session": "^1.16.1",
    "express-validator": "^5.3.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.0",
    "path": "^0.12.7",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^5.8.6"
  },
  "devDependencies": {
    "jasmine": "^3.4.0",
    "request": "^2.83.0"
  }
}
